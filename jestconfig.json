{
  "automock": false,
  "testRegex": ".+_test\\.tsx?$",
  "testPathIgnorePatterns": [
    "/.*\/bin/",
    "/.*\/node_modules/"
  ],
  "testURL": "http://localhost",
  "moduleFileExtensions": [
    "js",
    "json",
    "jsx",
    "ts",
    "tsx"
  ],
  "moduleNameMapper": {
    ".*(css|less|png|svg)$": "<rootDir>/jest_css_import_stub.js"
  },
  "setupFiles": [
    "./setUpJestTests.js"
  ],
  "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
  "transform": {
    ".*tsx?$": "./jest_preprocessor.js"
  },
  "verbose": true
}
